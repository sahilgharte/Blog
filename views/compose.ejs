<%- include('partials/header')-%>


<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="Title">Title</label>
    <input id="Title" class="form-control" type="text" name="postTitle">
  </div>
  <div class="form-group">
    <label for="Post">Post</label>
    <textarea name="postBody" class="form-control" id="Post" rows="6"></textarea>
  </div>
  <button class="btn btn-info" type="submit" name="button">Publish</button>
</form>

<%- include('partials/footer')-%>